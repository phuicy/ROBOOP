# Roboop Makefile for Visual C++ 
# $Id: makefile.vcpp,v 1.11 2006/08/17 17:36:17 gourdeau Exp $
#


#
# Visual C++ tools
#
CC   = cl 
LINK = link
AR  = lib

#
# Options
#
CFLAGS = -Inewmat -Isource -GX -O2 -D_STANDARD_

#
# Implicit rules
#
{source}.cpp.obj:
  $(CC)  $(CFLAGS) -c -o$@ $<

{newmat}.cpp.obj:
  $(CC)  $(CFLAGS) -c -o$@ $<

#
# Dependency List
#
deproboop = \
   demo.exe\
   demo_2dof_pd.exe\
   bench.exe\
   rtest.exe

roboop : $(deproboop)
  echo MakeNode 

depdemodexe = \
   roboop.lib\
   newmat.lib\
   source\demo.cpp

demo.exe : $(depdemodexe) source\utils.h source\robot.h source\quaternion.h source\gnugraph.h
    $(CC)  $(CFLAGS) -Fe$@ source\demo.cpp roboop.lib newmat.lib user32.lib

deproboopdlib = \
   gnugraph.obj\
   comp_dq.obj\
   comp_dqp.obj\
   delta_t.obj\
   dynamics.obj\
   homogen.obj\
   invkine.obj\
